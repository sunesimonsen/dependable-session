{
  "type": "module",
  "name": "@dependable/session",
  "version": "0.3.0",
  "description": "Save and restore @dependable/state to the session storage",
  "module": "./src/session.js",
  "exports": {
    ".": "./src/session.js"
  },
  "types": "./types/state.d.ts",
  "files": ["dist", "src", "types"],
  "keywords": ["dependable", "observable", "state", "session"],
  "scripts": {
    "test": "mocha",
    "format": "prettier --write '**/*.js'",
    "lint": "prettier --check '**/*.js' && eslint .",
    "types": "tsc",
    "build": "rm -rf dist types && npm run types && rollup -c rollup.config.js",
    "coverage": "c8 -r lcov -r text npm test",
    "api-docs": "npm run types && typedoc --out api-docs --hideLegend --hideGenerator src/session.js",
    "publish-docs": "npm run api-docs && surge api-docs dependable-session-api.surge.sh",
    "prepublishOnly": "npm run build"
  },
  "author": "Sune Simonsen",
  "license": "MIT",
  "bugs": "https://github.com/sunesimonsen/dependable-session/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunesimonsen/dependable-session"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "c8": "^7.11.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fake-promise": "^2.5.44",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "rollup": "^2.27.1",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^14.0.0",
    "surge": "^0.23.1",
    "terser": "^5.14.0",
    "typedoc": "^0.23.14",
    "typescript": "^4.7.3",
    "unexpected": "^13.0.0",
    "unexpected-dependable": "^1.1.0",
    "unexpected-sinon": "^11.1.0"
  },
  "dependencies": {
    "@dependable/state": "^0.17.0"
  }
}
